{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFPracticeVaibhav"
		},
		"LinkedService_BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService_BlobStorage'"
		},
		"LinkedService_OnPremFile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LinkedService_OnPremFile'"
		},
		"LinkedService_OnPremFile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\vaibhav.s\\Desktop\\Personal Learning\\Azure\\Practice Data"
		},
		"LinkedService_OnPremFile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "vaibhav.s@hcl.com"
		},
		"LinkedService_KV_Dev_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-ADF-Practice.vault.azure.net/"
		},
		"LinkedService_BlobStorage_KeyVault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Secret-AzurePractice"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFPracticeVaibhav_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedService_BlobStorage_connectionString": {
						"value": "[parameters('LinkedService_BlobStorage_connectionString')]"
					},
					"LinkedService_OnPremFile_password": {
						"value": "[parameters('LinkedService_OnPremFile_password')]"
					},
					"LinkedService_KV_Dev_properties_typeProperties_baseUrl": {
						"value": "[parameters('LinkedService_KV_Dev_properties_typeProperties_baseUrl')]"
					},
					"LinkedService_OnPremFile_properties_typeProperties_host": {
						"value": "[parameters('LinkedService_OnPremFile_properties_typeProperties_host')]"
					},
					"LinkedService_OnPremFile_properties_typeProperties_userId": {
						"value": "[parameters('LinkedService_OnPremFile_properties_typeProperties_userId')]"
					},
					"LinkedService_BlobStorage_KeyVault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LinkedService_BlobStorage_KeyVault_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFPracticeVaibhav_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFPracticeVaibhav_ArmTemplate_0')]"
			]
		}
	]
}